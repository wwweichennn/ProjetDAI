<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>ProjetDAI</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
<servlet>
    <servlet-name>index</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>ficheAppel</servlet-name>
    <jsp-file>/ficheAppel.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>emploiDuTemps</servlet-name>
    <jsp-file>/EmploiDuTemps.jsp</jsp-file>
</servlet>

<servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>ficheAppel</servlet-name>
    <url-pattern>/ficheAppel</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>emploiDuTemps</servlet-name>
    <url-pattern>/emploiDuTemps</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>testPhoto</servlet-name>
    <jsp-file>/testPhoto.jsp</jsp-file>
</servlet>

<servlet-mapping>
    <servlet-name>testPhoto</servlet-name>
    <url-pattern>/testPhoto</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>res</servlet-name>
    <jsp-file>/res.jsp</jsp-file>
</servlet>

<servlet-mapping>
    <servlet-name>res</servlet-name>
    <url-pattern>/res</url-pattern>
</servlet-mapping>






  
</web-app>