<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>ProjetDAI</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  
<servlet>
    <servlet-name>CrtlLogin</servlet-name>
    <servlet-class>crtl.CrtlLogin</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>CrtlLogin</servlet-name>
    <url-pattern>/CrtlLogin</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/Login.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>ficheAppel</servlet-name>
    <jsp-file>/ficheAppel.jsp</jsp-file>
</servlet>

<servlet>
    <servlet-name>emploiDuTemps</servlet-name>
    <jsp-file>/EmploiDuTemps.jsp</jsp-file>
</servlet>

<servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>ficheAppel</servlet-name>
    <url-pattern>/ficheAppel</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>emploiDuTemps</servlet-name>
    <url-pattern>/emploiDuTemps</url-pattern>
</servlet-mapping>
  
</web-app>